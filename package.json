{
  "name": "native-minimal",
  "version": "1.0.1",
  "description": "Native-minimal it's a CSS framework that uses simple native HTML tags. So you don't need to learn it at all.",
  "main": "dist/native-minimal.css",
  "scripts": {
    "start": "webpack serve --open --config webpack.demo.js --mode development",
    "build": "webpack --config webpack.build-demo.js --mode production",
    "build:framework": "webpack --config webpack.build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Golosay/native-minimal.git"
  },
  "keywords": [
    "css",
    "css-framework",
    "ui-framework",
    "dark-theme",
    "dark-mode",
    "lightweight",
    "minimal",
    "minimalist",
    "minimalistic",
    "native-html",
    "scss-framework",
    "semantic"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Golosay/native-minimal/issues"
  },
  "homepage": "https://native-minimal.web.app",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.3",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-fix-style-only-entries": "^0.6.1"
  }
}
